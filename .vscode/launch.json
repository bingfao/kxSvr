    "configurations": [
        {
            "name": "(Windows) Launch active file",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${fileDirname}\\${fileBasenameNoExtension}.exe",
            "args": ["127.0.0.1","10086"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "console": "externalTerminal"
        }
        ,
        {
            "name": "(Windows) Launch kxsvr",
